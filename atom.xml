<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Tom Preston-Werner</title>
 <link href="http://benjamin.thouret.com/atom.xml" rel="self"/>
 <link href="http://benjamin.thouret.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://benjamin.thouret.com/</id>
 <author>
   <name>Benjamin Thouret</name>
   <email>ben@2ret.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://benjamin.thouret.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://http:benjamin.thouret.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
